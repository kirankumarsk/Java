import java.util.*;

class Solution {
	public static void main(String[] args) {
		Scanner var = new Scanner(System.in);
		
		System.out.println("Enter a number of elements in array");
		int x = var.nextInt();
		
		int array[]= new int [x];
		
		for(int i=0;i<x;i++)
		{
			array[i]=var.nextInt();
		}
		
		System.out.println("Printing the array....");
		System.out.println(Arrays.toString(array));
	}
}
